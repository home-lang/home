// Integration test for try-catch-finally statements

// Basic try-catch
let result1 = 0;
try {
    result1 = 42;
} catch {
    result1 = 0;
}
assert(result1 == 42);

// Try-catch with error variable
try {
    let x = 10;
    let y = x + 5;
} catch (error) {
    print("Error occurred");
}

// Try-catch-finally
let finallyCalled = false;
try {
    let a = 5;
    let b = 10;
} catch {
    print("Caught error");
} finally {
    finallyCalled = true;
}
assert(finallyCalled == true);

// Finally always executes
let counter = 0;
try {
    counter = counter + 1;
} finally {
    counter = counter + 10;
}
assert(counter == 11);

// Nested try-catch
try {
    try {
        let x = 100;
    } catch {
        print("Inner catch");
    }
} catch {
    print("Outer catch");
}

print("âœ“ All try-catch-finally tests passed!");
