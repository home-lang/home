// Integration test for null coalescing operator

// Basic null coalescing
let name1 = "Alice";
let defaultName = "Unknown";
let result1 = name1 ?? defaultName;

// Chained null coalescing
let val1 = 0;
let val2 = 0;
let val3 = 42;
let result2 = val1 ?? val2 ?? val3;

// Null coalescing with expressions
fn getValue(hasValue: bool): int {
    return hasValue ? 100 : 0;
}

let value = getValue(false) ?? 50;

